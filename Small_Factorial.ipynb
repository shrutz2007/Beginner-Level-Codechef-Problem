        "t=int(input())\n",
        "def fact(n):\n",
        "    if n==1:\n",
        "        return 1\n",
        "    else: \n",
        "        return n*fact(n-1)\n",
        "for i in range(t):\n",
        "    n=int(input())\n",
        "    print(fact(n))"
